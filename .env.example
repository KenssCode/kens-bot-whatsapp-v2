# Bot WhatsApp Jual Beli - Environment Variables
# Copy file ini ke .env dan isi dengan nilai yang sesuai

# Database PostgreSQL (dari Railway)
DATABASE_URL=postgresql://postgres:password@localhost:5432/botwa

# Session Name untuk Baileys
SESSION_NAME=bot-jualbeli

# Prefix Command
COMMAND_PREFIX=.

# Fee Rekber (dalam persentase)
FEE_PERCENTAGE=1

# Nama Grup (opsional)
GROUP_NAME=Jual Beli

# Log Level (debug, info, warn, error)
LOG_LEVEL=info

